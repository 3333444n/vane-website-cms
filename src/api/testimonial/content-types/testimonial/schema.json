{
  "kind": "collectionType",
  "collectionName": "testimonials",
  "info": {
    "singularName": "testimonial",
    "pluralName": "testimonials",
    "displayName": "⭐️ Testimonial",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "quote": {
      "type": "richtext",
      "required": true
    },
    "authorName": {
      "type": "string",
      "required": true
    },
    "authorTitle": {
      "type": "string",
      "required": true
    },
    "order": {
      "type": "integer",
      "required": true
    },
    "page": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "homepage",
        "about",
        "both"
      ]
    },
    "target_page": {
      "type": "enumeration",
      "required": false,
      "enum": [
        "inicio",
        "sobre-mi",
        "curso"
      ]
    },
    "curso": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::curso.curso",
      "inversedBy": "testimonials"
    }
  }
}
